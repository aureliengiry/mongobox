{% extends "MongoboxJukeboxBundle:Videos:layout.html.twig" %}

{% block body %}
<h1>Videos creation</h1>

<form action="{{ path('videos_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('videos') }}" class="btn">
            Back to the list
        </a>
    </li>
</ul>
{% endblock body %}
