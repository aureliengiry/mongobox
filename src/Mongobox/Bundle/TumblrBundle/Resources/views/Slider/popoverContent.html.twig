<div id="vote-mongo">
	{% include 'MongoboxTumblrBundle:Slider:vote.html.twig' %}
</div>
<div class="tags-diffusion">
	{% for tag in tumblr.tags %}
		<span class="badge badge-warning">{{ tag.name }}</span>
	{% endfor %}
</div>
<div class="img-tumblr-big">
	<img src="{% if tumblr.localPath is null %}{{ tumblr.image }}{% else %}{{ tumblr.localPath }}{% endif %}" alt="{{ tumblr.text }}" title="{{ tumblr.text }}" />
</div>
<div class="groups-diffusion">
	{% for group in tumblr.groups %}
		<span class="badge badge-info">{{ group.title }}</span>
	{% endfor %}
</div>
